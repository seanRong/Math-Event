function checkPass4()
{
    var message4 = document.getElementById('confirmMessage4');
    var goodColor = "#66cc66";
    var badColor = "#ff6666";
    
	if(answer4.value == "50%"){
		tag5.style.display = 'block';
		answer4.style.backgroundColor = goodColor;
		message4.style.color = goodColor;
        message4.innerHTML = "That is correct!"
	}
	else if(answer4.value == "0.5"){
		tag5.style.display = 'block';
		answer4.style.backgroundColor = goodColor;
		message4.style.color = goodColor;
        message4.innerHTML = "That is correct!"
	}
	else if(answer4.value == "1/2"){
		tag5.style.display = 'block';
		answer4.style.backgroundColor = goodColor;
		message4.style.color = goodColor;
        message4.innerHTML = "That is correct!"
	}
	else{
		answer4.style.backgroundColor = badColor;
		message4.style.color = badColor;
        message4.innerHTML = "Incorrect. Please try again."
		}
}  

